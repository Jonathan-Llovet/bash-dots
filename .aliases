# CLI Helpers
alias cl='clear'
alias ll='ls -la'
alias ..='cd ..'
alias cd..='cd ..'
alias ...='cd ..'
alias now='date +"%T"'
alias sl='ls'
alias lsd="find . -maxdepth 1 -not -type d"

# Git
alias gl1='git log --oneline'
alias gcam='git commit -a -m'
alias gs='git status'
alias ga='git add .'
alias gcm='git commit -m'
alias gd='git diff'
alias gds='git diff --staged'
alias gp='git push'

# Python
alias python="python3"
alias py="python"
alias pip="pip3"
